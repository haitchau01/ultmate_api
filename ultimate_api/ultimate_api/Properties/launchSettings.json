{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    /* Bình luận nhiều dòng



    */
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:10648",
      "sslPort": 44390
    }
  },
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "launchUrl": "weatherforecast",
      "applicationUrl": "http://localhost:5149",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": false,
      "launchUrl": "weatherforecast",
      "applicationUrl": "https://localhost:7259;http://localhost:5149",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "IIS Express": {
      //run for vsual studio"
      "commandName": "IISExpress",
      "launchBrowser": false,
      "launchUrl": "weatherforecast",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "Docker": {
      //run for docker
      "commandName": "Docker",
      "launchBrowser": true,
      "httpPort": 5000,
      "useSSL": true,
      "environmentVariables": {
        "DOTNET_USE_POLLING_FILE_WATCHER": "1"
        /* Enable file change monitoring.*/

      }
    },
    "MyExternalApp": {
      //run for for .exe
      "commandName": "Executable",
      "executablePath": "C:\\Program Files\\MyApp\\MyApp.exe",
      "workingDirectory": "C:\\Program Files\\MyApp",
      "commandLineArgs": "--option1 --option2"
    },
    "MiddlewareExample": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "weatherforecast",
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}